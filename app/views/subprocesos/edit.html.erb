<h1>Editing subproceso</h1>

<% form_for(@subproceso) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :estado %><br />
    <%= f.text_field :estado %>
  </p>
  <p>
    <%= f.label :proceso %><br />
    <%= f.text_field :proceso %>
  </p>
  <p>
    <%= f.label :nombre_subproceso %><br />
    <%= f.text_field :nombre_subproceso %>
  </p>
  <p>
    <%= f.label :usuario_id_created %><br />
    <%= f.text_field :usuario_id_created %>
  </p>
  <p>
    <%= f.label :usuario_id_updated %><br />
    <%= f.text_field :usuario_id_updated %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @subproceso %> |
<%= link_to 'Back', subprocesos_path %>